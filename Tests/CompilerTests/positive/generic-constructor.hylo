//! no-std

struct A<T> {
  public memberwise init
  public fun f(x: sink T) -> T { x }
}

struct B<T, U> {
  public memberwise init
  public let x: T
  public let y: U
}

fun check<T>(_ x: T) {}

fun main() {
  let a0 = A<Void>()
  check<A<Void>>(a0)

  let a1 = A().f(x: ())
  check<Void>(a1)

  let b0 = B(x: (), y: ((), ()))
  check<B<Void, Void * Void>>(b0)
}
